#ifndef CALENDER_H_
#define CALENDER_H_
#include<iostream>
#include <ctime>
#include <iomanip>
using namespace std;
class Calender {
public:
	Calender();
	void Display(int, int );
	void printCal();
	friend bool LeapYear(int );
	friend int Daynum(int ,int,int );
	friend int noDays(int,int);
	friend string nameMonth(int);
	virtual ~Calender();
};

#endif /* CALENDER_H_ */

