#ifndef EHR_H_
#define EHR_H_
#include <string>
using namespace std;
class EHR {
	string consultation;
	string appointments[4];
	string prescribtions;
	string recommendations;
	string dietry_restriction;
	string allergies;
	string regular_medication;
	string chronic_health_condition;
public:
	EHR(string ="\0",string []=NULL,string="\0",string ="\0");
	virtual ~EHR();
};

#endif /* EHR_H_ */
