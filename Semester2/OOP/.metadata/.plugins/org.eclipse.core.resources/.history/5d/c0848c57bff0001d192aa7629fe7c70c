#ifndef SRC_CALL_H_
#define SRC_CALL_H_

class Call {
	int call_id;
	char notes_doc[300];
	char patient_doc[300];
	bool audio_vid;			//0 audio 1 video
	bool call_rec;
public:
	Call(int =0,char []="\0",char []="\0",bool =0,bool =0);
	virtual ~Call();
};

#endif /* SRC_CALL_H_ */
