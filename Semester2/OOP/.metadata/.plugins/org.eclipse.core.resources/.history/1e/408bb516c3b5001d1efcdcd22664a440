#include <iostream>
using namespace std;
int fun(int *n,int size,int start=0);


int main()
{
	int *n=new int [5];
	for(int i=0;i<5;i++)

		cin>>(*(n+i));
   cout<< fun(n,5);
    return 0;
}
int fun(int *n,int size,int start)
{
	if(start==size-1)
	{
		return 0;
	}
	else if(*(n+start)>*(n+0))
		{
			int res=fun(n+start,size,start+1);
			if(res==0)
			{
				return res;
			}
			else
			{
				return *(n+start);
			}
		}

	else
	{
		return fun(n,size,start+1);
	}

}
